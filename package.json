{
  "name": "mirror-protocol-price-feed",
  "version": "1.0.0",
  "main": "src/server.js",
  "license": "MIT",
  "private": true,
  "author": "crypt0grapher",
  "description": "Price Feed for mAssets from Mirror Protocol on Terra Blockchain with Node.js, Express.js, MongoDB, CSV Export, Docker Container",
  "keywords": [
    "blockchain",
    "terra",
    "mirror",
    "nodejs",
    "typescript",
    "tsc",
    "mongodb",
    "mongoose",
    "js",
    "javascript",
    "csv",
    "csv2json",
    "csv-export",
    "docker",
    "docker-image",
    "docker-compose",
    "Plug'n'Play"
  ],
  "scripts": {
    "watch-ts": "tsc -w",
    "watch-js": "nodemon ./dist/server.js",
    "build": "tsc",
    "start-app": "yarn watch-ts && yarn watch-js",
    "dev": "ts-node ./src/server.ts",
    "start": "node ./dist/server.js",
    "production": "tsc && yarn start"
  },
  "installConfig": {
    "pnp": true
  },
  "dependencies": {
    "@mirror-protocol/mirror.js": "^2.1.3",
    "@terra-money/terra.js": "^3.0.0",
    "@types/express": "^4.17.13",
    "@types/json2csv": "^5.0.3",
    "@types/pug": "^2.0.5",
    "body-parser": "^1.19.0",
    "csvtojson": "^2.0.10",
    "express": "^4.17.1",
    "json2csv": "^5.0.6",
    "mirror.js": "^0.0.1",
    "mongoose": "^6.0.13",
    "pug": "^3.0.2"
  },
  "devDependencies": {
    "typescript": "^4.5.2"
  },
  "engines": {
    "node": ">=16.11.6"
  }
}
